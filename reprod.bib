@Article{shivakumar_computations_2009,
  Title                    = {Computations of {Absolute} {Solvation} {Free} {Energies} of {Small} {Molecules} {Using} {Explicit} and {Implicit} {Solvent} {Model}},
  Author                   = {Shivakumar, Devleena and Deng, Yuqing and Roux, Beno{\^\i}t},
