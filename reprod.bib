@Article{martin_calculating_1998,
  Title                    = {Calculating {Gibbs} free energies of transfer from {Gibbs} ensemble {Monte} {Carlo} simulations},
  Author                   = {Martin, Marcus G. and Siepmann, J. Ilja},
  Journal                  = {Theoretical Chemistry Accounts},
