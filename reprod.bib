@Article{zafar_hydration_2016,
  Title                    = {Hydration {Free} {Energy} as a {Molecular} {Descriptor} in {Drug} {Design}: {A} {Feasibility} {Study}},
  Author                   = {Zafar, Ayesha and Reynisson, J{\'o}hannes},
  Journal                  = {Molecular Informatics},
  Year                     = {2016},

  Month                    = may,
  Number                   = {5},
  Pages                    = {207--214},
  Volume                   = {35},

  Abstract                 = {In this work the idea was investigated whether calculated hydration energy (ΔGhyd) can be used as a molecular descriptor in defining promising regions of chemical space for drug design. Calculating ΔGhyd using the Density Solvation Model (SMD) in conjunction with the density functional theory (DFT) gave an excellent correlation with experimental values. Furthermore, calculated ΔGhyd correlates reasonably well with experimental water solubility (r2=0.545) and also log P (r2=0.530). Three compound collections were used: Known drugs (n=150), drug-like compounds (n=100) and simple organic compounds (n=140). As an approximation only molecules, which do not de/protonate at physiological pH were considered. A relatively broad distribution was seen for the known drugs with an average at −15.3 kcal/mol and a standard deviation of 7.5 kcal/mol. Interestingly, much lower averages were found for the drug-like compounds (−7.5 kcal/mol) and the simple organic compounds (−3.1 kcal/mol) with tighter distributions; 4.3 and 3.2 kcal/mol, respectively. This trend was not observed for these collections when calculated log P and log S values were used. The considerable greater exothermic ΔGhyd average for the known drugs clearly indicates in order to develop a successful drug candidate value of ΔGhyd{\textless}−5 kcal/mol or less is preferable.},
  Bdsk-url-1               = {http://onlinelibrary.wiley.com/doi/10.1002/minf.201501035/abstract},
  Bdsk-url-2               = {http://dx.doi.org/10.1002/minf.201501035},
  Doi                      = {10.1002/minf.201501035},
  File                     = {Full Text PDF:/Users/guilhermematos/Library/Application Support/Zotero/Profiles/vfw1ip8r.default/zotero/storage/PAT3BRHQ/Zafar and Reynisson - 2016 - Hydration Free Energy as a Molecular Descriptor in.pdf:application/pdf;Snapshot:/Users/guilhermematos/Library/Application Support/Zotero/Profiles/vfw1ip8r.default/zotero/storage/98XRC4Z7/abstract.html:text/html},
  ISSN                     = {1868-1751},
  Keywords                 = {B3LYP, chemical space, chemoinformatics and drug design, density solvation model},
  Language                 = {en},
  Shorttitle               = {Hydration {Free} {Energy} as a {Molecular} {Descriptor} in {Drug} {Design}},
  Url                      = {http://onlinelibrary.wiley.com/doi/10.1002/minf.201501035/abstract},
  Urldate                  = {2016-08-19}
}

