@Article(JCROSS1986198,
  title =	 "Influence of hamiltonian parameterization on
                  convergence of kirkwood free energy calculations",
  author =	 "Albert J. Cross",
  journal =	 "Chem. Phys. Lett.",
  year =	 1986,
  number =	 2,
  pages =	 "198 - 202",
  volume =	 128,
  abstract =	 "Free energy differences between similar systems can
                  be calculated by an integration technique due to
                  Kirkwood, in which a Hamiltonian is constructed
                  which varies continuously through functions
                  intermediate between the initial and final functions
                  according to the value of an independent
                  parameter. A calculation for Lennard-Jones
