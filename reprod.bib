  File                     = {Full Text PDF:/Users/guilhermematos/Library/Application Support/Zotero/Profiles/vfw1ip8r.default/zotero/storage/9W9CAKZ7/Bieler and H{\"u}nenberger - 2015 - Orthogonal sampling in free-energy calculations of.pdf:application/pdf;Snapshot:/Users/guilhermematos/Library/Application Support/Zotero/Profiles/vfw1ip8r.default/zotero/storage/5AUG4DQC/abstract.html:text/html},
  ISSN                     = {1096-987X},
  Keywords                 = {alchemical free-energy calculations, thermodynamic integration, λ-dynamics, λ-local-elevation umbrella-sampling},
  Language                 = {en},
  Shorttitle               = {Orthogonal sampling in free-energy calculations of residue mutations in a tripeptide},
  Url                      = {http://onlinelibrary.wiley.com/doi/10.1002/jcc.23984/abstract},
  Urldate                  = {2016-08-15}
}

@Article{bodnarchuk_water_2016,
