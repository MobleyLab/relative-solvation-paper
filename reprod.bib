  ISSN                     = {1352-2310},
  Keywords                 = {Activity coefficient, Atmospheric aerosol, Organic/electrolyte/water, Water activity},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S135223100800188X},
  Urldate                  = {2016-08-10}
}

