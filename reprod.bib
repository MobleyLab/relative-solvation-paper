@Article(Horn:2004:TheJournalofChemicalPhysics,
  title =	 "Development of an Improved Four-Site Water Model for
                  Biomolecular Simulations: {{TIP4P}}-{{Ew}}",
  author =	 "Horn, Hans W. and Swope, William C. and Pitera, Jed
                  W. and Madura, Jeffry D. and Dick, Thomas J. and
                  Hura, Greg L. and Head-Gordon, Teresa",
  journal =	 "J. Chem. Phys.",
  year =	 2004,
  month =	 may,
  number =	 20,
  pages =	 "9665--9678",
  volume =	 120,
  abstract =	 "A re-parameterization of the standard TIP4P water
                  model for use with Ewald techniques is introduced,
                  providing an overall global improvement in water
                  properties relative to several popular
                  nonpolarizable and polarizable water
                  potentials. Using high precision simulations, and
                  careful application of standard analytical
                  corrections, we show that the new TIP4P-Ew potential
                  has a density maximum at $\sim$1$\mkern1mu^\circ$C,
                  and reproduces experimental bulk-densities and the
                  enthalpy of vaporization, $\Delta$Hvap,$\Delta$Hvap,
                  from -37.5 to 127$\mkern1mu^\circ$C at 1 atm with an
                  absolute average error of less than 1\%. Structural
                  properties are in very good agreement with x-ray
                  scattering intensities at temperatures between 0 and
                  77$\mkern1mu^\circ$C and dynamical properties such
                  as self-diffusion coefficient are in excellent
                  agreement with experiment. The parameterization
                  approach used can be easily generalized to
                  rehabilitate any water force field using available
                  experimental data over a range of thermodynamic
                  points.",
  doi =		 "10.1063/1.1683075",
  file =	 "Full Text PDF:/Users/dmobley/Library/Application
                  Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/U7D6C28V/Horn
                  et al. - 2004 - Development of an improved four-site
                  water model
                  f.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application
                  Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/QG48KNIR/1.html:text/html",
  issn =	 "0021-9606",
  shorttitle =	 "Development of an Improved Four-Site Water Model for
                  Biomolecular Simulations",
  timestamp =	 "2017-01-20T00:49:48Z",
  urldate =	 "2017-01-20"
)

@Article(Hu2016,
  title =	 "The importance of protonation and tautomerization in
                  relative binding affinity prediction: a comparison
                  of AMBER TI and Schr{\"o}dinger FEP",
  author =	 "Hu, Yuan and Sherborne, Brad and Lee, Tai-Sung and
                  Case, David A. and York, Darrin M. and Guo, Zhuyan",
  journal =	 "J. Comput. Aided Mol. Des. ",
  year =	 2016,
  number =	 7,
  pages =	 "533--539",
  volume =	 30,
  abstract =	 "In drug discovery, protonation states and
                  tautomerization are easily overlooked. Through a
                  Merck--Rutgers collaboration, this paper re-examined
                  the initial settings and preparations for the
                  Thermodynamic Integration (TI) calculation in AMBER
                  Free-Energy Workflows, demonstrating the value of
                  careful consideration of ligand protonation and
                  tautomer state. Finally, promising results comparing
                  AMBER TI and Schr{\"o}dinger FEP+ are shown that
                  should encourage others to explore the value of TI
                  in routine Structure-based Drug Design.",
