@Article{haranczyk_solvation_2008,
  Title                    = {Solvation free energies of molecules. {The} most stable anionic tautomers of uracil.},
  Author                   = {Haranczyk, Maciej and Gutowski, Maciej and Warshel, Arieh},
  Journal                  = {Physical chemistry chemical physics : PCCP},
