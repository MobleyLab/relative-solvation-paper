  Author                   = {McGrath, Matthew J. and Kuo, I.-F. Will and Ngouana W, Brice F. and Ghogomu, Julius N. and Mundy, Christopher J. and Marenich, Aleksandr V. and Cramer, Christopher J. and Truhlar, Donald G. and Siepmann, J. Ilja},
