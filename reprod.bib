@Article{heras_genetic_2016,
  Title                    = {Genetic {Algorithms} for {Digital} {Quantum} {Simulations}},
  Author                   = {Heras, U. Las and Alvarez-Rodriguez, U. and Solano, E. and Sanz, M.},
