  ISSN                     = {1607324X},
  Keywords                 = {Condensed Matter - Soft Condensed Matter},
  Shorttitle               = {Molecular theory of solvation},
  Url                      = {http://arxiv.org/abs/1510.06520},
  Urldate                  = {2016-08-12}
