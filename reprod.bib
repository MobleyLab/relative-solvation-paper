@Article{westergren_silico_2007,
  Title                    = {In silico prediction of drug solubility: 1. {Free} energy of hydration},
  Author                   = {Westergren, Jan and Lindfors, Lennart and H{\"{o}}glund, Tobias and {L\"{u}}der, Kai and Nordholm, Sture and Kjellander, Roland},
  Journal                  = {The Journal of Physical Chemistry. B},
  Year                     = {2007},

  Month                    = feb,
  Number                   = {7},
  Pages                    = {1872--1882},
  Volume                   = {111},

  Abstract                 = {As a first step in the computational prediction of drug solubility the free energy of hydration, DeltaG*(vw) in TIP4P water has been computed for a data set of 48 drug molecules using the free energy of perturbation method and the optimized potential for liquid simulations all-atom force field. The simulations were performed in two steps, where first the Coulomb and then the Lennard-Jones interactions between the solute and the water molecules were scaled down from full to zero strength to provide physical understanding and simpler predictive models. The results have been interpreted using a theory assuming DeltaG*(vw) = A(MS)gamma + E(LJ) + E(C)/2 where A(MS) is the molecular surface area, gamma is the water-vapor surface tension, and E(LJ) and E(C) are the solute-water Lennard-Jones and Coulomb interaction energies, respectively. It was found that by a proper definition of the molecular surface area our results as well as several results from the literature were found to be in quantitative agreement using the macroscopic surface tension of TIP4P water. This is in contrast to the surface tension for water around a spherical cavity that previously has been shown to be dependent on the size of the cavity up to a radius of approximately 1 nm. The step of scaling down the electrostatic interaction can be represented by linear response theory.},
  Bdsk-url-1               = {http://dx.doi.org/10.1021/jp064220w},
  Date-modified            = {2017-01-11 18:57:03 +0000},
  Doi                      = {10.1021/jp064220w},
  ISSN                     = {1520-6106},
  Keywords                 = {Computer simulation, Hydrogen bonding, Molecular conformation, Pharmaceutical Preparations, Solubility, Solvents, Surface Tension, Thermodynamics, Water},
  Language                 = {eng},
  Pmid                     = {17266351},
  Shorttitle               = {In silico prediction of drug solubility}
}

@Article{wickstrom_parameterization_2016,
  Title                    = {Parameterization of an effective potential for protein--ligand binding from host--guest affinity data},
  Author                   = {Wickstrom, Lauren and Deng, Nanjie and He, Peng and Mentes, Ahmet and Nguyen, Crystal and Gilson, Michael K. and Kurtzman, Tom and Gallicchio, Emilio and Levy, Ronald M.},
