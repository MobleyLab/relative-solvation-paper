@Article{loeffler_fesetup:_2015,
  Title                    = {{FESetup}: {Automating} {Setup} for {Alchemical} {Free} {Energy} {Simulations}},
  Author                   = {Loeffler, Hannes H. and Michel, Julien and Woods, Christopher},
  Journal                  = {Journal of Chemical Information and Modeling},
