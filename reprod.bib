@Article{zuckerman_systematic_2004,
  Title                    = {Systematic {Finite}-{Sampling} {Inaccuracy} in {Free} {Energy} {Differences} and {Other} {Nonlinear} {Quantities}},
  Author                   = {Zuckerman, Daniel and Woolf, Thomas},
  Journal                  = {Journal of Statistical Physics},
  Year                     = {2004},
  Number                   = {5},
  Pages                    = {1303--1323},
  Volume                   = {114},

  Abstract                 = {Systematic inaccuracy is inherent in any computational estimate of a non-linear average, such as the free energy difference ?F between two states or systems, because of the availability of only a finite number of data values, N. In previous work, we outlined the fundamental statistical description of this ``finite-sampling error.'' We now give a more complete presentation of (i) rigorous general bounds on the free energy and other nonlinear averages, which generalize Jensen's inequality; (ii) asymptotic N?8 expansions of the average behavior of the finite-sampling error in ?F estimates; (iii) illustrative examples of large-N behavior, both in free-energy and other calculations; and (iv) the universal, large-N relation between the average finite-sampling error and the fluctuation in the error. An explicit role is played by L{\'e}vy and Gaussian limiting distributions.},
  Bdsk-url-1               = {http://www.springerlink.com/content/r95681130n278gk4/abstract/$%5Cbackslash$nhttp://www.springerlink.com/content/r95681130n278gk4/fulltext.pdf},
  Bdsk-url-2               = {http://dx.doi.org/10.1023/B:JOSS.0000013961.84860.5b},
  Doi                      = {10.1023/B:JOSS.0000013961.84860.5b},
  ISSN                     = {0022-4715},
  Keywords                 = {energy computation, finite-sampling error, free, jensen, nonlinear averages, s inequality, systematic error},
  Url                      = {http://www.springerlink.com/content/r95681130n278gk4/abstract/$\backslash$nhttp://www.springerlink.com/content/r95681130n278gk4/fulltext.pdf}
}

